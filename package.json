    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "gray-matter": "^4.0.3",
    "next": "15.0.2",
    "next-mdx-remote": "^5.0.0",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "react-icons": "^5.3.0",
    "react-share": "^5.1.0",
    "rehype-pretty-code": "^0.13.2",
